   "source": [
    "import tensorflow as tf\n",
    "\n",
    "#\n",
    "a = tf.constant(120, name=\"a\")\n",
    "b = tf.constant(130, name=\"b\")\n",
    "c = tf.constant(140, name=\"c\")\n",
    "\n",
    "#\n",
    "v = tf.Variable(0, name=\"v\")\n",
    "\n",
    "#\n",
    "calc_op = a + b + c\n",
    "assign_op = tf.assign(v, calc_op)\n",
    "\n",
    "#\n",
    "sess = tf.Session()\n",
    "sess.run(assign_op)\n",
    "\n",
    "#\n",
    "print(sess.run(v))"
   ]
  }
